<!DOCTYPE html>
<html lang="en">

<head>
    <link href="/styles/register-styles.css" rel="stylesheet">
    <%- include('./partials/head.ejs'); %>
</head>

<body>
    <header class="main-header">
        <%- include('./partials/header.ejs'); %>
    </header>
    <main>
        <div>
            <img src="/images/users/<%= user.image %>" alt="<%= user.image %>">
        </div>
        <div>
            <h3><%= user.first_name %></h3>
            <p><%= user.last_name %></p>
            <p><%= user.email %></p>
            <p><%= user.category %></p>
    </main>
    <footer>
        <%- include('./partials/footer.ejs'); %>
    </footer>
</body>
</html>